{
    "$schema": "https://railway.app/railway.schema.json",
    "build": {
        "builder": "nixpacks",
        "buildCommand": "cd DevNet/Server && npm install && npm run build"
    },
    "deploy": {
        "startCommand": "cd DevNet/Server && npm start",
        "restartPolicyType": "ON_FAILURE"
    }
}